                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <TableView fx:id="tableview" layoutX="273.0" layoutY="211.0" prefHeight="699.0" prefWidth="1080.0">
         <columns>
            <TableColumn fx:id="tid" prefWidth="105.0" text="ID Commande" />
            <TableColumn fx:id="tidproduit" prefWidth="85.0" text="ID Produit" />
            <TableColumn fx:id="tuser" prefWidth="85.0" text="User Name" />
            <TableColumn fx:id="tdatecom" prefWidth="72.0" text="Date Commande" />
            <TableColumn fx:id="tprixtotal" minWidth="0.0" prefWidth="0.0" text="prixtotale" />
            <TableColumn fx:id="tvalide" prefWidth="182.0" text="Valide/En cours" />
         </columns>
      </TableView>
   </children>
